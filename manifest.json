{
  "manifest_version": 2,
  "name": "Sort Outlook Calendars",
  "description": "Sort list of calendars on the Outlook web app",
  "homepage_url": "https://github.com/jasonku/sort-outlook-calendars",
  "version": "1.0",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },

  "content_scripts": [
    {
      "matches": ["*://outlook.office.com/*"],
      "js": ["./improvements.js"]
    }
  ]
}
